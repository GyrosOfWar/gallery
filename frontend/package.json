{
  "name": "imagehive-frontend",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./css/app.css -o ./app/css/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./css/app.css -o ./app/css/app.css",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint app && prettier --check .",
    "test": "vitest"
  },
  "dependencies": {
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/v1-route-convention": "^0.1.4",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "flowbite": "^1.7.0",
    "flowbite-react": "^0.9.0",
    "imagehive-client": "file:../imagehive-client",
    "immer": "^9.0.21",
    "isbot": "^3.6.13",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-avatar": "^5.0.3",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-icons": "^4.10.1",
    "react-infinite-scroll-hook": "^4.1.1",
    "react-leaflet": "^4.2.1",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.5.0",
    "remix-utils": "^6.6.0",
    "tus-js-client": "^3.1.0",
    "ua-parser-js": "^1.0.37",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/leaflet": "^1.9.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.45.0",
    "jsdom": "^21.1.1",
    "msw": "^2.3.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=14"
  }
}
